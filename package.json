{
	"name": "meteor",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"seed": "prisma db seed"
	},
	"devDependencies": {
		"@material/animation": "^14.0.0",
		"@material/button": "^14.0.0",
		"@material/density": "^14.0.0",
		"@material/dialog": "^14.0.0",
		"@material/elevation": "^14.0.0",
		"@material/focus-ring": "^14.0.0",
		"@material/icon-button": "^14.0.0",
		"@material/ripple": "^14.0.0",
		"@material/rtl": "^14.0.0",
		"@material/shape": "^14.0.0",
		"@material/theme": "^14.0.0",
		"@material/tokens": "^14.0.0",
		"@material/touch-target": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@playwright/test": "^1.22.2",
		"@smui/button": "^6.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/d3": "^7.4.0",
		"@types/node": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.1.0",
		"smui-theme": "^6.0.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@prisma/client": "^4.1.0",
		"cookie": "^0.4.1",
		"d3": "^7.6.1"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	}
}